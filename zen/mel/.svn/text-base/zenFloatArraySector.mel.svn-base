//return an array which is comprised of the portion
//beginning at $start and ending at $end

global proc float[] zenFloatArraySector(float $array[], int $start,int $end)
{
	int 	$n=0;
	float 	$newArray[];

		for($i=$start;$i<=$end;$i++)
		{
			$newArray[$n]=$array[$i];
			$n++;
		}

	return 	$newArray;
}